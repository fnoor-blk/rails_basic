<center><h1>Listing Blogs</h1></center>
<%@blogs.each do |blog|%>
<div class="card">
	<div class="card-header">
		<%="<h1>#{blog.title}</h1> <small> by #{blog.author.email}</small>".html_safe%>
	</div>
	<div class="card-body">
		<%=blog.description%>
	</div>
	<div class="card-footer">
		<%="#{time_ago_in_words(blog.created_at)} ago"%>
	</div>
</div>
<br><br><br>
<%end%>