<%=form_for data[:blog],url:data[:url] do |f|%>
	<div class="form-group">
		<%= f.label :title%>
		<%= f.text_field :title,class:"form-control"%>
	</div>
	<div class="form-group">
		<%= f.label :description%>
		<%= f.cktext_area :description,class:"form-control"%>
	</div>
	<div class="form-group">
		<%= f.submit "Submit",class:"btn btn-outline-success"%>
	</div>
<%end%>